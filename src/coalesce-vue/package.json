{
  "name": "coalesce-vue",
  "version": "0.1.0-local",
  "description": "Coalesce core scripts for Vue.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./lib/*": {
      "types": "./lib/*.d.ts",
      "import": "./lib/*.js",
      "require": "./lib/cjs/*.js"
    }
  },
  "repository": "https://github.com/IntelliTect/Coalesce",
  "author": "IntelliTect",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "build": "tsc -p ./tsconfig.build.esm.json && tsc -p ./tsconfig.build.cjs.json",
    "prepack": "npm run build",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "files": [
    "lib",
    "src",
    "test",
    "tsconfig.json",
    "tsconfig.build.json"
  ],
  "dependencies": {
    "@types/lodash-es": "^4.17.3",
    "axios": "^0.26.1",
    "date-fns": "2.28.0",
    "date-fns-tz": "1.3.3",
    "lodash-es": "^4.17.15",
    "magic-string": "^0.26.2",
    "tslib": "2.3.1"
  },
  "peerDependencies": {
    "vue": "^2.7.0-alpha.8 || ^3.2.37"
  },
  "optionalDependencies": {
    "vue-router": ">=3.0.0 || ^4.0.16"
  },
  "devDependencies": {
    "@vue/test-utils": "^2.0.0",
    "cross-blob": "^1.2.1",
    "lodash": "^4.17.15",
    "typescript": "4.6.3",
    "vite": "2.9.12",
    "vitest": "^0.15.0",
    "vue": "3.2.34",
    "vue-router": "4.0.16"
  }
}
